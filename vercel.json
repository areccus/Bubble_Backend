{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "env": {
    "MONGO_URL": "@mongo_url",
    "GCLOUD_STORAGE_KEYFILE": "@gcloud_storage_keyfile",
    "NODE_ENV": "production"
  }
}